  "main": "dist/travian.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/anwinged/travian.git"
  },
  "scripts": {
    "test": "mocha",
    "test:junit-report": "mocha --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/results.xml",
    "coverage": "nyc mocha",
    "build:dev": "webpack",
    "build": "webpack --env.production",
    "format": "prettier --tab-width=4 --print-width 120 --single-quote --trailing-comma es5 --write '{src,tests}/**/*.{ts,js}'",
    "format-check": "prettier --tab-width=4 --print-width 120 --single-quote --trailing-comma es5 --check '{src,tests}/**/*.{ts,js}'",
    "format-vue": "prettier --tab-width=2 --print-width 120 --single-quote --trailing-comma es5 --write '{src,tests}/**/*.vue'",
    "format-wp": "prettier --tab-width=4 --print-width 120 --single-quote --trailing-comma es5 --write 'webpack.config.js'"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/dateformat": "^3.0.1",
    "@types/debounce": "^1.2.0",
    "@types/jquery": "^3.3.34",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.4",
    "@types/url-parse": "^1.4.3",
    "chai": "^4.2.0",
    "css-loader": "^3.5.2",
    "dateformat": "^3.0.3",
    "debounce": "^1.2.0",
    "jquery": "^3.4.1",
    "mocha": "^7.1.1",
    "mocha-junit-reporter": "^1.23.3",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.0",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "url-parse": "^1.4.7",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.1.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {}
}
